{
  "name": "rokka/kirby",
  "description": "Rokka plugin for Kirby CMS",
  "type": "kirby-plugin",
  "license": "MIT",
  "homepage": "https://github.com/rokka-io/rokka-kirby-plugin",
  "authors": [
    {
      "name": "The rokka devs (https://rokka.io)",
      "email": "rokka@rokka.io"
    }
  ],
  "require": {
    "rokka/client": "^1.14.0",
    "getkirby/cms": "^3.6"
  },
  "autoload": {
    "psr-4": {
      "Rokka\\Kirby\\": "src/Kirby"
    },
    "classmap": ["src/Rokka.php"],
    "files": ["config.php"]
  },
  "config": {
    "optimize-autoloader": true,
    "allow-plugins": {
      "getkirby/composer-installer": true
    }
  },
  "extra": {
    "installer-name": "rokka"
  },
  "keywords": [
    "kirby3",
    "kirby3-cms",
    "kirby3-plugin",
    "rokka",
    "image",
    "imagemanipulation",
    "rokka.io"
  ]
}
